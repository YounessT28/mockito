pipeline
{
    agent any
    tools
    {
        	maven 'maven'
            jdk 'java-11'
    }
    stages
    {
        stage ('Initialize')
        {
                steps {
                    echo "maven"
                    echo "jdk"
                }
        }
        stage('Build')
        {
            steps
            {
                sh 'mvn clean package'
            }
            post
            {
                success
                {
                    junit '**/target/surefire-reports/*.xml'
                }
            }

        }
    }
}